{
	"name": "storybook",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"eslint": "8.34.0",
		"svelte": "^3.54.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.31.2",
		"@storybook/addon-essentials": "7.0.0-rc.1",
		"@storybook/addon-interactions": "7.0.0-rc.1",
		"@storybook/addon-links": "7.0.0-rc.1",
		"@storybook/blocks": "7.0.0-rc.1",
		"@storybook/svelte": "7.0.0-rc.1",
		"@storybook/sveltekit": "7.0.0-rc.1",
		"@storybook/testing-library": "0.0.14-next.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.11.0",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"@turboswind/eslint-config": "workspace:*",
		"@turboswind/prettier": "workspace:*",
		"eslint-plugin-storybook": "^0.6.11",
		"publint": "^0.1.10",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"storybook": "7.0.0-rc.1",
		"svelte": "^3.56.0",
		"svelte-check": "^3.1.2",
		"tslib": "^2.5.0",
		"typescript": "5.0.0-beta",
		"vite": "^4.1.4",
		"vitest": "^0.29.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
