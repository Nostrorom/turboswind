		"svelte": "^3.56.0",
		"svelte-check": "^3.1.0",
		"tslib": "^2.5.0",
		"typescript": "5.0.0-beta",
		"vite": "^4.1.4",
		"vitest": "^0.29.2"
	},
	"type": "module",
	"peerDependencies": {
		"eslint": "8.34.0",
